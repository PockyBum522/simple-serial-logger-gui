{
    "$schema": "http://json.schemastore.org/template",
    "author": "PockyBum522 (David S)",
    "classifications": [ "GUI", "WPF", "Windows", "MVVM" ],
    "identity": "PockyBum522.Wpf.GuiTemplate",
    "name": "PockyBum522 WPF Solution",
    "description": "PockyBum522 New WPF Template with Dependency injection and MVVM",
    "shortName": "pockywpfmvvm",
    "sourceName": "SimpleSerialLoggerGui",
    "tags": {
        "language": "C#",
        "type": "solution"
    },
    "preferNameDirectory": true,
    "sources": [
        {
            "source": "./",
            "target": "./",
            "include": [ "**/*" ],
            "exclude": [
                "**/[Bb]in/**",
                "**/[Oo]bj/**",
                "**/.template.config",
                "**/*.filelist",
                "**/*.user",
                "**/*.lock.json",
                "**/.git/**",
                "**/*.nuspec",
                "**/.idea/**"
            ],
            "copyOnly":[
                "**/node_modules/**"
            ]
        }
    ]
}